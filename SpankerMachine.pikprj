[General]
description=Control a spanker machine to allow a more interesting spanking experience
output=SpankerMachine.hex
type=asm
version=0.1

[Assembler]
include-dir=$(PRJPATH)
list=false
other-options=
radix=dec
target-device=p16f628
warn-level=2

[Files]
inputFiles=SpankerMachine.inc, main.asm, wait.asm, lcd.asm, key.asm, motor.asm, display.asm

[Linker]
debug=false
hex-format=inhx32
map=true
objs-libs-dir=
other-options=
