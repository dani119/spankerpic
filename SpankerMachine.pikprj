[General]
description=Control a spanker machine to allow a more interesting spanking experience
output=SpankerMachine.hex
version=0.1
type=asm

[Assembler]
target-device=p16f628
warn-level=2
other-options=
include-dir=$(PRJPATH)
radix=dec
list=false

[Linker]
hex-format=inhx32
debug=false
map=true
objs-libs-dir=
other-options=

[Files]
inputFiles=taster.asm, SpankerMachine.inc, interrupt.asm, main.asm, wait.asm
